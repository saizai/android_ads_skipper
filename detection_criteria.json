{
  "ad_skip_buttons": {
    "prefix": "fn:reverse((.//*[@android:clickable='true' and @android:enabled='true' and (",
    "postfix": ")])[1]/ancestor-or-self::*)",
    "identifiers": [
      {
        "id": ".*:id/cbb"
      },
      {
        "id": "com.google.android.apps.youtube.music:id/dismiss(_button)?"
      },
      {
        "id": ".*:id/banderol_close"
      },
      {
        "id": ".*:id/skip_ad(_button)?(_text|_container)?"
      },
      {
        "text": "[cC]lose [aA]d( panel)?"
      },
      {
        "text": "(Pass|Ignor)er l(es |')annonces?"
      },
      {
        "text": "[sS][kK][iI][pP] [aA][dD][sS]?"
      },
      {
        "text": "略過廣告"
      },
      {
        "text": "Werbung(en)? überspringen"
      }
    ]
  },
  "ad_playing_indicators": {
    "prefix": "fn:reverse((.//*[@android:enabled='true' and (",
    "text_prefix": "(fn:matches(@android:contentDescription|@android:text[not(../@android:editable='true')],\"^(",
    "identifiers": [
    {
      "id": "gmsg-video-player"
    },
    {
      "id": "performance_video_container"
    },
    {
      "id": "count-down"
    },
    {
      "id": "com.google.android.apps.youtube.music:id/player_learn_more_button"
    },
    {
      "text": "Visit advertiser"
    },
      {"text": "[sS][kK][iI][pP] [aA][dD][sS]?( in [0-9]+.*)?"},
    {
      "id": "com.google.android.youtube:id/countdown_text"
    },
    {
      "id": ".*:id/ad_progress_text"
    },
    {
      "id": ".*:id/app_promo_ad_cta_overlay"
    }
    ]
  },
  "extra_clickables": [
    {
      "id": "com.google.android.youtube:id/hide_controls_button_top"
    },
    {
      "id": "close_(video_)?button"
    }
  ],
  "ad_playing_indicators_xpath": {
    "id":"(fn:matches(@android:id,'^@.*:id/(skip_ad_text|skip_ad_button_container|skip_ad_button_text|skip_ad_button|banderol_close|
    app_promo_ad_cta_overlay|ad_progress_text)$')) or 
    (fn:matches(@android:id,'^@(com.google.android.apps.youtube.music:id/dismiss|com.google.android.apps.youtube.music:id/dismiss_button|com.google.android.youtube:id/countdown_text|com.google.android.apps.youtube.music:id/player_learn_more_button|count-down|performance_video_container|gmsg-video-player)"
  }
}
